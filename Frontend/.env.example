# ===========================================
# FRONTEND ENVIRONMENT CONFIGURATION TEMPLATE
# ===========================================
# Copy this file to .env and fill in your values

# Environment
NODE_ENV=development
VITE_ENV=development

# Backend API Configuration
VITE_API_BASE_URL=http://localhost:5000/api
VITE_BACKEND_URL=http://localhost:5000

# Zoom SDK Configuration
VITE_ZOOM_SDK_KEY=your_zoom_sdk_key_here
VITE_ZOOM_SDK_SECRET=your_zoom_sdk_secret_here

# Meeting Configuration
VITE_DEFAULT_MEETING_DURATION=60
VITE_DEFAULT_ATTENDANCE_THRESHOLD=75

# Feature Flags
VITE_DEBUG_ZOOM=true
VITE_ENABLE_WEBSOCKETS=true

# Legacy React App variables for backwards compatibility (if needed)
REACT_APP_ZOOM_SDK_KEY=your_zoom_sdk_key_here
REACT_APP_API_BASE_URL=http://localhost:5000/api
