# Frontend Production Environment Variables

# API Configuration
REACT_APP_API_BASE_URL=http://localhost/api
REACT_APP_SOCKET_URL=http://localhost

# Application Settings
REACT_APP_ENV=production
REACT_APP_DEFAULT_MEETING_DURATION=60
REACT_APP_DEFAULT_ATTENDANCE_THRESHOLD=75

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_DEBUG_MODE=false

# Zoom SDK Configuration (if needed on frontend)
REACT_APP_ZOOM_SDK_KEY=your-zoom-sdk-key
REACT_APP_ZOOM_SDK_SECRET=your-zoom-sdk-secret

# Other API URLs (if you have external services)
# REACT_APP_EXTERNAL_API_URL=https://api.external-service.com
